        section.o-sectionCategories
            .container
                h1.o-sectionCategories__title Vybrané kategorie
                .row

                    - for (var i=0; i<3; i++)
                        .col.o-sectionCategories__col
                            a.o-sectionCategories__single.m-categoryCell.b-bgCover__wrap(href="#")
                                img.m-categoryCell__img.b-bgCover__img(src=URLbase+"img/product-thumb-medium.jpg" alt="#" title="#")
