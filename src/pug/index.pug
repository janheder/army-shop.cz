        section.o-sectionProducts.o-tabs

            nav.o-tabs__nav.m-tabsNav
                .container  
                    .nav.nav-tabs.m-tabsNav__nav(role="tablist")
                
                        a.m-tabsNav__link.active(id="index-1-tab" data-toggle="tab" href="#index-1" role="tab" aria-controls="index-1" aria-selected="true") Doporučené
                
                        a.m-tabsNav__link(id="index-2-tab" data-toggle="tab" href="#index-2" role="tab" aria-controls="index-2" aria-selected="false") Novinky
            
                        a.m-tabsNav__link(id="index-3-tab" data-toggle="tab" href="#index-3" role="tab" aria-controls="index-3" aria-selected="false") Nejprodávanější

            .tab-content.o-tabs__content.m-tabsContent

                .tab-pane.fade.m-tabsContent__pane.show.active(id="index-1" role="tabpanel" aria-labelledby="index-1-tab")  
                    .m-tabsContent__collapseToggle(data-toggle="collapse" data-target="#collapseIndexTab-1" aria-expanded="true" aria-controls="#collapseIndexTab-1") Doporučené
                    .collapse.m-tabsContent__collapse.show(id="collapseIndexTab-1" aria-labelledby="collapseIndexTab-1")
                        .container  
                            .o-productGrid
                                include partials/_single-product-cell.pug
                                include partials/_single-product-cell.pug
                                include partials/_single-product-cell.pug
                                include partials/_single-product-cell.pug
                        .m-tabsContent__btnWrap  
                            a.btn(href="#") Celá nabídka #[span.a-btnIcon #[span.a-icon.-cheveron.-white]]


                .tab-pane.fade.m-tabsContent__pane(id="index-2" role="tabpanel" aria-labelledby="index-2-tab")    
                    .m-tabsContent__collapseToggle(data-toggle="collapse" data-target="#collapseIndexTab-2" aria-expanded="false" aria-controls="#collapseIndexTab-2") Novinky
                    .collapse.m-tabsContent__collapse(id="collapseIndexTab-2" aria-labelledby="collapseIndexTab-2")
